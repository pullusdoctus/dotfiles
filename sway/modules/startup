# | Apps |
# +------+

# Workspace 1
exec swaymsg "workspace 1; exec $term"
# Workspace 10
exec swaymsg "workspace 10; exec $musplayer"
# Minimized
for_window [app_id="$torrent"] floating enable, move scratchpad
exec swaymsg "exec $torrent"
